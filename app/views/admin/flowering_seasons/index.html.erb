<h1>新規投稿</h1>
<!-- form_with部分 -->

<%= form_with model: @flowering_season, url: admin_flowering_seasons_path, local:true do |f| %>


  <h4>カテゴリー</h4>
  <%= f.text_field :season %>



  <%= f.submit '投稿' %>

  <% end %>
<div>
  <p>開花時期</p>
  <% @flowering_seasons.each do |flowering_season| %>
  <p><%= flowering_season.season %><%= link_to "編集する", edit_admin_flowering_season_path(flowering_season) %></p>
</div>


<% end %>